<% if flash[:alert]%> 
  <% flash.each do |name, msg|%>
    <div>
      <div class='alert alert-danger alert-dismissable fade show' role='alert'>
        <%= msg %>
        <button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='Close'></button>
      </div>
    </div>
  <% end %>
<% end %>